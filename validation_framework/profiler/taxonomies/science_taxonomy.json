        "qudt:Turbidity": {
          "qudt_id": "qudt:Turbidity",
          "definition": "Cloudiness or haziness of fluid",
          "display_label": "Turbidity",
          "patterns": [
            "(?i)^turbidity$",
            "(?i)^.*_turbidity$",
            "(?i)^clarity$",
            "(?i)^haze$",
            "(?i)^ntu$"
