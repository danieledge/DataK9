{
  "metadata": {
    "version": "1.0.0",
    "description": "Scientific measurement semantic taxonomy for DataK9 profiler",
    "sources": [
      {
        "name": "QUDT (Quantities, Units, Dimensions and Types)",
        "url": "https://qudt.org/",
        "license": "CC BY 4.0",
        "license_url": "https://creativecommons.org/licenses/by/4.0/"
      },
      {
        "name": "ChEBI (Chemical Entities of Biological Interest)",
        "url": "https://www.ebi.ac.uk/chebi/",
        "license": "CC BY 4.0",
        "license_url": "https://creativecommons.org/licenses/by/4.0/"
      },
      {
        "name": "UO (Units of Measurement Ontology)",
        "url": "https://github.com/bio-ontology-research-group/unit-ontology",
        "license": "CC BY 3.0",
        "license_url": "https://creativecommons.org/licenses/by/3.0/"
      }
    ],
    "created": "2025-12-07",
    "note": "Patterns derived from QUDT/ChEBI/UO ontologies for offline use. No runtime API calls required."
  },
  "taxonomy": {
    "chemistry": {
      "description": "Chemical compounds, elements, and biochemistry",
      "tags": {
        "chebi:Alcohol": {
          "chebi_id": "CHEBI:30879",
          "definition": "Organic compound with hydroxyl functional group",
          "display_label": "Alcohol content",
          "patterns": [
            "(?i)^alcohol$",
            "(?i)^.*_alcohol$",
            "(?i)^alcohol_.*$",
            "(?i)^ethanol$",
            "(?i)^.*ethanol.*$"
          ],
          "data_properties": {
            "type": ["float", "decimal"],
            "min_value": 0,
            "max_value": 100
          },
          "unit_hint": "% v/v or g/L",
          "priority": 85
        },
        "chebi:OrganicAcid": {
          "chebi_id": "CHEBI:64709",
          "definition": "Organic compound with acidic properties",
          "display_label": "Organic acid",
          "patterns": [
            "(?i)^.*_acid$",
            "(?i)^.*acid$",
            "(?i)^malic_?acid$",
            "(?i)^citric_?acid$",
            "(?i)^acetic_?acid$",
            "(?i)^lactic_?acid$",
            "(?i)^tartaric_?acid$",
            "(?i)^ascorbic_?acid$",
            "(?i)^fatty_?acid.*$",
            "(?i)^amino_?acid.*$",
            "(?i)^acidity$",
            "(?i)^.*_acidity$",
            "(?i)^fixed_acidity$",
            "(?i)^volatile_acidity$",
            "(?i)^total_acidity$"
          ],
          "data_properties": {
            "type": ["float", "decimal"],
            "min_value": 0
          },
          "unit_hint": "g/L or mg/L",
          "priority": 82
        },
        "chebi:Phenol": {
          "chebi_id": "CHEBI:33853",
          "definition": "Aromatic compound with hydroxyl group attached to benzene ring",
          "display_label": "Phenolic compound",
          "patterns": [
            "(?i)^.*phenol.*$",
            "(?i)^.*phenolic.*$",
            "(?i)^total_phenols$",
            "(?i)^polyphenols$"
          ],
          "data_properties": {
            "type": ["float", "decimal"],
            "min_value": 0
          },
          "unit_hint": "mg/L GAE",
          "priority": 80
        },
        "chebi:Flavonoid": {
          "chebi_id": "CHEBI:47159",
          "definition": "Polyphenolic plant secondary metabolite",
          "display_label": "Flavonoid",
          "patterns": [
            "(?i)^flavon.*$",
            "(?i)^.*flavon.*$",
            "(?i)^flavanoid.*$",
            "(?i)^.*flavanoid.*$",
            "(?i)^anthocyanin.*$",
            "(?i)^.*anthocyanin.*$",
            "(?i)^tannin.*$",
            "(?i)^.*tannin.*$"
          ],
          "data_properties": {
            "type": ["float", "decimal"],
            "min_value": 0
          },
          "unit_hint": "mg/L",
          "priority": 80
        },
        "chebi:Ash": {
          "chebi_id": "CHEBI:46662",
          "definition": "Inorgite mineral residue after combustion",
          "display_label": "Ash content",
          "patterns": [
            "(?i)^ash$",
            "(?i)^.*_ash$",
            "(?i)^ash_.*$",
            "(?i)^total_ash$",
            "(?i)^alkalinity_of_ash$",
            "(?i)^ash_alkalinity$"
          ],
          "data_properties": {
            "type": ["float", "decimal"],
            "min_value": 0
          },
          "unit_hint": "g/L or %",
          "priority": 78
        },
        "chebi:Mineral": {
          "chebi_id": "CHEBI:46662",
          "definition": "Inorganic element or compound",
          "display_label": "Mineral",
          "patterns": [
            "(?i)^magnesium$",
            "(?i)^calcium$",
            "(?i)^potassium$",
            "(?i)^sodium$",
            "(?i)^iron$",
            "(?i)^zinc$",
            "(?i)^copper$",
            "(?i)^manganese$",
            "(?i)^phosphorus$",
            "(?i)^sulfur$",
            "(?i)^chloride$",
            "(?i)^.*_mg$",
            "(?i)^.*_ca$",
            "(?i)^.*_k$",
            "(?i)^.*_na$",
            "(?i)^.*_fe$"
          ],
          "data_properties": {
            "type": ["float", "decimal", "integer"],
            "min_value": 0
          },
          "unit_hint": "mg/L or ppm",
          "priority": 75
        },
        "chebi:Sugar": {
          "chebi_id": "CHEBI:16646",
          "definition": "Carbohydrate compound",
          "display_label": "Sugar content",
          "patterns": [
            "(?i)^sugar.*$",
            "(?i)^.*sugar$",
            "(?i)^.*_sugar$",
            "(?i)^glucose$",
            "(?i)^fructose$",
            "(?i)^sucrose$",
            "(?i)^lactose$",
            "(?i)^residual_sugar$",
            "(?i)^total_sugar.*$"
          ],
          "data_properties": {
            "type": ["float", "decimal"],
            "min_value": 0
          },
          "unit_hint": "g/L",
          "priority": 78
        },
        "chebi:Protein": {
          "chebi_id": "CHEBI:36080",
          "definition": "Biological macromolecule composed of amino acids",
          "display_label": "Protein",
          "patterns": [
            "(?i)^protein.*$",
            "(?i)^.*protein$",
            "(?i)^.*_protein$",
            "(?i)^total_protein$",
            "(?i)^albumin$",
            "(?i)^globulin$"
          ],
          "data_properties": {
            "type": ["float", "decimal"],
            "min_value": 0
          },
          "unit_hint": "g/L or mg/dL",
          "priority": 76
        },
        "chebi:Lipid": {
          "chebi_id": "CHEBI:18059",
          "definition": "Hydrophobic biomolecule",
          "display_label": "Lipid/Fat",
          "patterns": [
            "(?i)^fat$",
            "(?i)^.*_fat$",
            "(?i)^lipid.*$",
            "(?i)^.*lipid$",
            "(?i)^cholesterol$",
            "(?i)^triglyceride.*$",
            "(?i)^saturated_fat$",
            "(?i)^unsaturated_fat$",
            "(?i)^trans_fat$"
          ],
          "data_properties": {
            "type": ["float", "decimal"],
            "min_value": 0
          },
          "unit_hint": "g or mg/dL",
          "priority": 76
        },
        "chebi:Sulfite": {
          "chebi_id": "CHEBI:26836",
          "definition": "Sulfur dioxide and related compounds",
          "display_label": "Sulfite",
          "patterns": [
            "(?i)^.*sulfite.*$",
            "(?i)^.*sulphite.*$",
            "(?i)^so2$",
            "(?i)^.*_so2$",
            "(?i)^free_sulfur.*$",
            "(?i)^total_sulfur.*$",
            "(?i)^sulfur_dioxide$",
            "(?i)^sulphur_dioxide$"
          ],
          "data_properties": {
            "type": ["float", "decimal", "integer"],
            "min_value": 0
          },
          "unit_hint": "mg/L or ppm",
          "priority": 77
        },
        "chebi:Proline": {
          "chebi_id": "CHEBI:26271",
          "definition": "Amino acid found in proteins",
          "display_label": "Proline",
          "patterns": [
            "(?i)^proline$",
            "(?i)^.*_proline$"
          ],
          "data_properties": {
            "type": ["float", "decimal", "integer"],
            "min_value": 0
          },
          "unit_hint": "mg/L",
          "priority": 80
        }
      }
    },
    "physical_properties": {
      "description": "Physical measurements and properties",
      "tags": {
        "qudt:Density": {
          "qudt_id": "qudt:Density",
          "definition": "Mass per unit volume",
          "display_label": "Density",
          "patterns": [
            "(?i)^density$",
            "(?i)^.*_density$",
            "(?i)^specific_gravity$",
            "(?i)^sg$"
          ],
          "data_properties": {
            "type": ["float", "decimal"],
            "min_value": 0
          },
          "unit_hint": "g/mL or kg/m³",
          "priority": 80
        },
        "qudt:pH": {
          "qudt_id": "qudt:PH",
