            "(?i)^.*_glucose$",
            "(?i)^blood_sugar$",
            "(?i)^fasting_glucose$",
            "(?i)^hba1c$",
            "(?i)^a1c$"
          ],
          "data_properties": {
            "type": ["float", "decimal", "integer"],
            "min_value": 0
