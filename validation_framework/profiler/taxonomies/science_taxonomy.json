            "(?i)^.*_conc$",
            "(?i)^conc_.*$",
            "(?i)^.*_level$",
            "(?i)^.*_content$"
          ],
          "data_properties": {
            "type": ["float", "decimal"],
            "min_value": 0
          },
          "unit_hint": "mg/L, g/L, mol/L, or ppm",
          "priority": 75
        },
        "qudt:Absorbance": {
          "qudt_id": "qudt:Absorbance",
          "definition": "Measure of light absorption",
          "display_label": "Color/Absorbance",
          "patterns": [
            "(?i)^color$",
            "(?i)^colour$",
            "(?i)^.*_color$",
            "(?i)^.*_colour$",
            "(?i)^color_intensity$",
            "(?i)^colour_intensity$",
            "(?i)^absorbance$",
            "(?i)^optical_density$",
            "(?i)^od$",
            "(?i)^od_.*$",
            "(?i)^hue$"
          ],
          "data_properties": {
            "type": ["float", "decimal"],
            "min_value": 0
