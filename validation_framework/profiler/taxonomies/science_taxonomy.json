            "(?i)^body_temp.*$"
          ],
          "data_properties": {
            "type": ["float", "decimal", "integer"]
          },
          "unit_hint": "°C, °F, or K",
          "priority": 85
        },
        "qudt:Pressure": {
          "qudt_id": "qudt:Pressure",
          "definition": "Force per unit area",
          "display_label": "Pressure",
          "patterns": [
            "(?i)^pressure$",
            "(?i)^.*_pressure$",
            "(?i)^.*pressure$",
            "(?i)^bp$",
            "(?i)^systolic.*$",
            "(?i)^diastolic.*$",
            "(?i)^blood_pressure.*$",
            "(?i)^atmospheric_pressure$",
            "(?i)^bar$",
            "(?i)^psi$"
          ],
          "data_properties": {
            "type": ["float", "decimal", "integer"],
            "min_value": 0
          },
          "unit_hint": "Pa, bar, mmHg, or psi",
          "priority": 82
        },
        "qudt:Concentration": {
          "qudt_id": "qudt:MassConcentration",
          "definition": "Amount of substance per unit volume",
          "display_label": "Concentration",
          "patterns": [
