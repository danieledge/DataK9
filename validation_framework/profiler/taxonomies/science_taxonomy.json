        "qudt:Viscosity": {
          "qudt_id": "qudt:Viscosity",
          "definition": "Resistance to flow",
          "display_label": "Viscosity",
          "patterns": [
            "(?i)^viscosity$",
            "(?i)^.*_viscosity$",
            "(?i)^kinematic_viscosity$",
            "(?i)^dynamic_viscosity$"
          ],
          "data_properties": {
            "type": ["float", "decimal"],
            "min_value": 0
          },
          "unit_hint": "Pa·s or cP",
          "priority": 75
        },
        "qudt:Conductivity": {
          "qudt_id": "qudt:ElectricConductivity",
          "definition": "Ability to conduct electric current",
          "display_label": "Conductivity",
          "patterns": [
            "(?i)^conductivity$",
            "(?i)^.*_conductivity$",
            "(?i)^electrical_conductivity$",
            "(?i)^ec$"
          ],
          "data_properties": {
            "type": ["float", "decimal"],
            "min_value": 0
          },
          "unit_hint": "S/m or µS/cm",
          "priority": 75
        }
      }
    },
